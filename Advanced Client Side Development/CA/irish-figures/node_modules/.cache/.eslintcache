ter as Router, Switch, Route, link } from 'react-router-dom';\nimport MyNavbar from \"./components/navbar\";\nimport Home from './views/Home';\nimport Bram from './views/bram';\nimport Anne from './views/anne';\nimport Charles from './views/charles';\nimport Jocelyn from './views/jocelyn';\nimport Ernest from './views/ernest';\n\nfunction App() {\n  return (\n    <div className=\"App bg-secondary\">\n      <Router>\n        <div>\n          <MyNavbar/>\n          <Switch>\n            <Route exact path=\"/\">\n              <Home/>\n            </Route>\n            <Route path=\"/bram\">\n              <Bram/>\n            </Route>\n            <Route path=\"/anne\">\n              <Anne/>\n            </Route>\n            <Route path=\"/charles\">\n              <Charles/>\n            </Route>\n            <Route path=\"/jocelyn\">\n              <Jocelyn/>\n            </Route>\n            <Route path=\"/ernest\">\n              <Ernest/>\n            </Route>\n          </Switch>\n        </div>\n      <Footer/>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;","/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/serviceWorkerRegistration.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/anne.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/jocelyn.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/Home.js",["87"],[],"// Home page view. Format is quite consistent with the individuals' pages, but with the removal of the Books API element, and the Carousel instead of standard Header from other pages.\n\nimport MyCarousel from \"../components/myCarousel\";\n\nfunction Home() {\n    return (\n        <div className=\"container-fluid\" id=\"myCarousel\">\n            <MyCarousel/>\n            <div className=\"row justify-content\">\n                <div className=\"col-7 mx-auto\">\n                    <h2>Irish Historical Figures</h2>\n                    <p>When you add up the figures, over 10 million men and women left Ireland over the centuries. But Irish history is more than just a long list of names, dates and statistics. . .</p>\n                    <p>The interactive exhibitions at EPIC The Irish Emigration Museum explore why they left, as well as the lasting legacies of some famous Irish people in history. Hereâ€™s just a few your kids will want to hear about. . .</p><br/>\n                    <p>Find out more about EPIC: The Irish Immigration Museum <a className=\"linking\" href=\"http://epicchq.com/\" target=\"_blank\">here</a>.</p><br/>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Home;","/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/ernest.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/charles.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/components/footer.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/views/bram.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/components/navbar.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/components/myCarousel.js",[],[],"/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/components/header.js",["88"],[],"// Header component which can be used by each page to keep the design consistent across the site.\n\nfunction Header(props) {\n    return (\n        <header className=\"row banner\">\n            <div className=\"col p-0\">\n                <img className=\"img-fluid w-100\" id={props.shuffle} src={props.image}/>\n                <h1 className=\"d-block overlay\">{props.header}</h1>\n            </div>\n        </header>\n    )\n}\n\nexport default Header;","/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/components/books.js",["89","90","91","92","93"],[],"// Component for making and utilising an API call to the Google Books API\n\nimport { useState, useEffect } from \"react\";\nimport axios from 'axios';\n\nfunction Books(props) {\n    // Variables for retrieved data from the API call, setting a condition for what to actually return, and an error message.\n    const [bookInfo, setBookInfo] = useState([]);\n    const [used, setUsed] = useState(false);\n    const [err, setErr] = useState('')\n\n    useEffect(async function() {\n        try {\n            // API call to Google Books, including API Key generated from Google API, and a query received through props which signifies what person to look up\n            var data = await axios('https://www.googleapis.com/books/v1/volumes', {params: {q: props.query, key: 'AIzaSyD9AyhB8F--bj5fLXUp5fMnGxDy0VXLAt0'}});\n            // Storing the received data\n            setBookInfo(data.data.items);\n            // Marking if the API call was successful, so we know what to return later\n            setUsed(true);\n            console.log(data.data.items)\n        } catch(e) {\n            // Setting an error if the API call fails.\n            setErr(\"No Books Available\")\n        }\n    }, [])\n\n    // If Statement to return different results depending on whether or not the API call was successful\n    if (used) {\n        return (\n            <div>\n                <h3>Related Reading:</h3>\n                    {\n                        // Mapping the JSON entries to retrieve data from Arrays\n                        bookInfo.map(function(i, index) {\n                            // If statement to skip a book and not generate an entry if there is data missing from it\n                            if (!i.volumeInfo.imageLinks || !i.volumeInfo.infoLink || !i.volumeInfo.authors || !i.volumeInfo.publishedDate || !i.volumeInfo.description) {\n                                console.log(\"Book excluded for missing data\")\n                            } else {\n                                return (\n                                    // Generates an entry for each book received from the API call\n                                    <a className=\"row mb-1 rounded bg-light text-dark align-items-center book-box\" href={i.volumeInfo.infoLink} key={index} target=\"_blank\">\n                                        <div className=\"col-xl-2\">\n                                            <img className=\"border border-5 border-light\" src={i.volumeInfo.imageLinks.smallThumbnail}/>\n                                        </div>\n                                        <div className=\"col-xl-8\">\n                                            <div className=\"row\">\n                                                <h4>{i.volumeInfo.title} by {i.volumeInfo.authors[0]}</h4>\n                                            </div>\n                                            <div className=\"row text-muted\">\n                                                <p>Published: ({i.volumeInfo.publishedDate})</p>\n                                            </div>\n                                            <div className=\"row\">\n                                                <p>{i.volumeInfo.description}</p>\n                                            </div>\n                                        </div>\n                                    </a>\n                                )\n                            }\n                        })\n                    }\n            </div>\n        )\n    } else {\n        // Error returned if the API call failed.\n        return (\n            <div><h3>{err}</h3></div>\n        )\n    }\n}\n\nexport default Books;","/Users/richard.duffy/Code/NCIRL/Advanced Client Side Development/CA/irish-figures/src/service-worker.js",